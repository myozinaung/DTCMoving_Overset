FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale    1;

xmin    -0.8;           xmax    6.4;
ymin    -0.58;          ymax    0;
zmin    -0.25;          zmax    0.75;

vertices
(
    ($xmin $ymin $zmin)
    ($xmax $ymin $zmin)
    ($xmax $ymax $zmin)
    ($xmin $ymax $zmin)

    ($xmin $ymin $zmax)
    ($xmax $ymin $zmax)
    ($xmax $ymax $zmax)
    ($xmin $ymax $zmax)

);

blocks
(
    hex (0 1 2 3 4 5 6 7) ( 230 20 32 ) simpleGrading ( 1 1 1 )
);

edges
(
);

boundary
(
    oversetHull
    {
        type overset;
        faces
        (
            (0 4 7 3) // xmin
            (1 2 6 5) // xmax
            (0 1 5 4) // ymin
            // (3 7 6 2) // ymax
            (0 3 2 1) // bottom
            (4 5 6 7) // top
        );
    }

    hullMidPlane
    {
        type symmetry;
        faces
        (
            (3 7 6 2) // ymax
        );
    }
);


